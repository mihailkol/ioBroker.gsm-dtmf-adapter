{
    "common": {
        "name": "gsm-dtmf-adapter",
        "version": "0.0.1",
        "title": "GSM DTMF Adapter",
        "desc": {
            "en": "Adapter for controlling devices via GSM modem with DTMF",
            "de": "Adapter zur Steuerung von Geräten über GSM-Modem mit DTMF"
        },
        "platform": "Javascript/Node.js",
        "mode": "daemon",
        "icon": "gsm.png",
        "enabled": true,
        "extIcon": "https://iobroker.live/assets/gsm.png",
        "keywords": [
            "gsm",
            "dtmf",
            "modem"
        ],
        "readme": "https://github.com/your-repo/gsm-dtmf-adapter/blob/main/README.md",
        "materialize": true,
        "adminUI": {
            "config": "json"
        }
    },
    "native": {
      "modemPath": "/dev/ttyUSB0",
      "baudRate": 9600,
      "user1": {
        "name1": "",
        "phone1": ""
      },
      "user2": {
        "name2": "",
        "phone2": ""
      },
      "user3": {
        "name3": "",
        "phone3": ""
      }
    }
}
